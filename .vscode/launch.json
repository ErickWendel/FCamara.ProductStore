{
    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceRoot}",
			"name": "Launch TypeScript",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/src/presentation/API/Index.ts",
			"sourceMaps": true,
			"outDir": "${workspaceRoot}/lib"
		},
        
        {
			"name": "Attach to Node",
			"type": "node",
			"request": "attach",
			"address": "localhost",
			"port": 5858,
			"restart": false
		}
    ]
}